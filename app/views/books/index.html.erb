<h1>BooKs</h1>


<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
    </tr>
  </thead>
</table>



<h1>New Book</h1>
<%= form_with model: @book, url: 'books', method: :post do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>
	
  <h4>Body</h4>
  <%= f.text_area :body %>
	
  <%= f.submit '投稿' %>
<% end %>